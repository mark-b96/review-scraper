{
  "websites": {
    "amazon": {
      "scrape": {
        "url": "https://www.amazon.co.uk/product-reviews/$product_id/ref=cm_cr_getr_d_paging_btm_next_$page_num?ie=UTF8&reviewerType=all_reviews&pageNumber=$page_num",
        "reviews_per_page": 10
      },
      "search": {
        "url": "https://www.amazon.co.uk/s?k=$search_term",
        "search_spaces": "+",
        "parser": [
          [
            "div",
            "data-component-type",
            "s-search-result"
          ],
          [
            "data-asin"
          ]
        ]
      }
    },
    "johnlewis": {
      "scrape": {
        "url": "https://api.johnlewis.com/ratings-reviews/v1/reviews?productId=$product_id&pageNumber=$page_num&pageSize=$reviews_per_page&sort=newest&api_key=AIzaSyD0XVOmTWbXYigofMbhS9cSeG76BwQisO8",
        "reviews_per_page": 25,
        "parser": {"results_key": "reviews", "review_title": "title", "review_text": "text", "review_rating": ["overallRating", "value"], "review_location": ["reviewer", "location"] }
      },
      "search": {
        "url": "https://www.johnlewis.com/search?search-term=$search_term",
        "search_spaces": "+",
        "parser": [
          [
            "article",
            "data-test",
            "product-card"
          ],
          [
            "data-product-id"
          ]
        ]
      }
    },
    "buybuybaby": {
      "scrape": {
        "url": "https://buybuybaby.com/apis/services/conversations/review?ProductId=$product_id&rows=8&start=$page_num&sort=SubmissionTime%20desc&Stats=Reviews&&__amp_source_origin=https%3A%2F%2Fwww.buybuybaby.com",
        "reviews_per_page": 8,
        "parser": {"results_key": "Results", "review_title": "Title", "review_text": "ReviewText", "review_rating": "Rating", "review_location":  ""}
      },
      "search": {
        "url": "https://www.buybuybaby.com/apis/services/composite/product-listing/v1.0/all?web3feo=1&site=BuyBuyBaby&currencyCode=USD&country=US&rT=xtCompat&tz=0&displayAdsAt=6&web3feo=abc&q=$search_term&wt=json&badge_ids=7464&url=%2Fstore%2Fs&noFacet=false&facets=%7B%7D&start=0&perPage=24&sws=&storeOnlyProducts=false&customPriceRange=false&__amp_source_origin=https%3A%2F%2Fwww.buybuybaby.com",
        "search_spaces": "%20",
        "parser": [
          [
            "div",
            "data-cta class",
            "s12 t4 v025 height100 prodCardWrap"
          ],
          [
            "PRODUCT_ID"
          ]
        ]
      }
    }
  },
  "headers": {
    "pragma": "no-cache",
    "cache-control": "no-cache",
    "dnt": "1",
    "upgrade-insecure-requests": "1",
    "user-agent": "Mozilla/5.0 (Macintosh; Intel Mac OS X 10_10_1) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/96.0.4664.110 Safari/537.36",
    "accept": "text/html,application/xhtml+xml,application/xml;q=0.9,image/webp,image/apng,*/*;q=0.8,application/signed-exchange;v=b3;q=0.9",
    "sec-fetch-site": "none",
    "sec-fetch-mode": "navigate",
    "sec-fetch-dest": "document",
    "accept-language": "en-GB,en-US;q=0.9,en;q=0.8"
  },
  "max_review_pages": 50,
  "delay_between_requests": [1.3, 4.0]
}